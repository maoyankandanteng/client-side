{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [false],
        "semicolon": [false],
        "interface-name": [true,"never-prefix"],
        "object-literal-sort-keys": false,
        "max-classes-per-file": [false],
        "no-empty": [false],
        "ban-types": [false],
        "no-namespace": [false],
        "no-reference": false,
        "only-arrow-functions": false
    },
    "rulesDirectory": []
}
